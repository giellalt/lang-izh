:a SG-ILL_V   ; ! maihe
:a Oblique-Singular ;
! Plural
:a PL-NOM   ; ! maat
:o PL-PAR_tA  ; ! maita
:o PL-ILL_hV   ; ! maihe
:o Oblique-Plural ;

